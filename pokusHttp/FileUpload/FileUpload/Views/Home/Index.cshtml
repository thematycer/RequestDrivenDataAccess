@{
    ViewData["Title"] = "File Upload";
}

<h2>File Upload</h2>

<form action="/api/File" method="post" enctype="multipart/form-data">
    <div>
        <label>Username:</label>
        <input type="text" name="userName" required />
    </div>
    <div>
        <label>Upload Format:</label>
        <input type="text" name="uploadFormat" required />
    </div>
    <div>
        <label>Wanted Format:</label>
        <input type="text" name="wantedFormat" required />
    </div>
    <div>
        <label>Select File:</label>
        <input type="file" name="file" required />
    </div>
    <button type="submit">Upload</button>
</form>
<p>
    <a href="/Home/GetFile">Go to Get File</a>
</p>
